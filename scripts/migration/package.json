{
  "name": "cms-stripe-migration",
  "version": "1.0.0",
  "description": "Production-ready CMS to Stripe migration tool",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "migrate": "ts-node index.ts",
    "migrate:dry-run": "ts-node index.ts --dry-run",
    "migrate:resume": "ts-node index.ts --resume",
    "migrate:validate": "ts-node index.ts --validate",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "stripe": "^14.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": [
    "migration",
    "stripe",
    "supabase",
    "cms",
    "e-commerce"
  ],
  "author": "Your Team",
  "license": "MIT"
}