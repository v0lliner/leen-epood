{
  "name": "production-stripe-migration",
  "version": "1.0.0",
  "type": "module",
  "description": "Production-ready Stripe migration system with comprehensive error handling",
  "main": "index.js",
  "scripts": {
    "migrate": "node index.js",
    "migrate:dry-run": "node index.js --dry-run",
    "migrate:force": "node index.js --force-resync",
    "migrate:debug": "node index.js --log-level=DEBUG",
    "migrate:small-batch": "node index.js --batch-size=3",
    "test-connection": "node test-connection.js",
    "clear-checkpoints": "node clear-checkpoints.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "stripe": "^17.7.0",
    "dotenv": "^16.3.1"
  },
  "keywords": [
    "stripe",
    "migration",
    "supabase",
    "production",
    "fault-tolerant"
  ],
  "author": "Production Migration System",
  "license": "MIT"
}